{
    "name": "biurad/php-framework",
    "description": "Biurad PHP Framework for PHP - The PHP Framework for the future...",
    "keywords": [
        "micro framework",
        "biurad",
        "radion",
        "psr",
        "psr-7",
        "psr-11",
        "psr-15",
        "php framework"
    ],
    "license": "BSD-3-Clause",
    "type": "project",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "homepage": "https://biurad.com",
    "authors": [
        {
            "name": "Divine Niiquaye Ibok",
            "email": "divineibok@gmail.com",
            "homepage": "https://www.biurad.com"
        }
    ],
    "require": {
        "php": "^7.1.32",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-zip": "*",
        "defuse/php-encryption": "^2.2",
        "doctrine/annotations": "^1.8",
        "doctrine/cache": "^1.10",
        "doctrine/collections": "^1.6",
        "doctrine/inflector": "^1.3",
        "doctrine/instantiator": "^1.3",
        "eftec/bladeone": "^3.31",
        "egulias/email-validator": "^2.1",
        "erusev/parsedown": "^1.7.3",
        "erusev/parsedown-extra": "^0.8.1",
        "filp/whoops": "^2.7",
        "guzzlehttp/guzzle": "^6.5",
        "http-interop/http-factory-guzzle": "^1.0",
        "laminas/laminas-escaper": "^2.6",
        "laminas/laminas-httphandlerrunner": "^1.1",
        "laminas/laminas-hydrator": "^3.0",
        "laminas/laminas-stratigility": "^3.2",
        "league/flysystem": "^1.0",
        "league/flysystem-cached-adapter": "^1.0",
        "monolog/monolog": "^2.0",
        "nette/di": "^3.0",
        "nette/php-generator": "^3.3",
        "nette/robot-loader": "^3.2",
        "nette/schema": "^1.0",
        "nikic/php-parser": "^4.3",
        "ocramius/package-versions": "1.5",
        "psr/container": "^1.0",
        "psr/event-dispatcher": "^1.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/log": "^1.1",
        "psr/simple-cache": "^1.0",
        "rakit/validation": "^1.2",
        "spiral/pagination": "^2.2",
        "symfony/console": "^5.0",
        "symfony/dotenv": "^4.4",
        "symfony/options-resolver": "^5.0",
        "symfony/polyfill-ctype": "^1.13",
        "symfony/process": "^5.0",
        "symfony/security-csrf": "^4.3",
        "symfony/yaml": "^4.4",
        "tightenco/collect": "^6.4"
    },
    "require-dev": {
        "ext-libxml": "*",
        "ext-mongodb": "*",
        "ext-mbstring": "*",
        "ext-redis": "*",
        "ext-sqlite3": "*",
        "ext-zend-opcache": "*",
        "symfony/var-dumper": "^4.3"
    },
    "autoload": {
        "files": [
            "vendor/biurad/biurad-helpers/src/functions.php",
            "vendor/divineniiquaye/cycle-proxy-factory/src/functions/utils.php",
            "vendor/divineniiquaye/cycle-proxy-factory/src/functions/expressions.php",
            "vendor/divineniiquaye/cycle-proxy-factory/src/functions/version.php",
            "vendor/divineniiquaye/cycle-proxy-factory/src/functions/injector.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "app/tests/"
        },
        "exclude-from-classmap": [
            "vendor/divineniiquaye/cycle-proxy-factory/tests"
        ]
    },
    "extra": {
        "biurad": {
            "__defaults": {
                "dashboard-ui": true,
                "disable-errors": true,
                "check-requirements": true
            }
        }
    },
    "scripts": {
        "coverage-clover": "phpunit --colors=always --do-not-cache-result --coverage-clover clover.xml",
        "phpunit": "phpunit --colors=always --do-not-cache-result",
        "phpstan": "phpstan analyse --level 5 src",
        "serve":   "php biurad server:run"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    }
}
